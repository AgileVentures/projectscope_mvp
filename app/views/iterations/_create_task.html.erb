<%= form_for :task do |f| %>
    <table>
      <tbody>
        <tr>
          <%= f.label :name%>
          <%= f.text_field :name %>
        </tr>
        <tr>
          <%= f.label :updater_type%>
          <%= f.text_field :updater_type %>
        </tr>
        <tr>
          <%= f.label :description%>
          <%= f.text_field :description %>
        </tr>
      </tbody>
    </table>
    <%= f.submit %>
<% end %>