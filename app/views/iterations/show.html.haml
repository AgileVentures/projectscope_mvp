%div.col-md-10
  %h2
    Iteration
  %div
    = button_to 'update all', update_all_task_path
  %table.table
    %tbody{style: 'text-align: center'}
      - @level.each do |nodes|
        %tr{id: "task_#{@iteration}"}
          %td
          - nodes.each do |node|
            -if node.nil?
              %td
            -else
              = render partial: 'task', locals: {node: node, iteration: @iteration}
:javascript
  keep_log=false;