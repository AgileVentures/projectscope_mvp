
%h1
  = @iteration.name

%div.col-md-6
  %h2
    Tasks
  %table.table.table-striped
    %thead
      %tr
        -1.upto(@maxelem).each do |level|
          %th
            level #{level}
    %tbody
      - @level.each do |nodes|
        %tr{id: "task_#{@iteration}"}
          - nodes.each do |node|
            %td
              = Task.find(node).title

:javascript
  keep_log=false;