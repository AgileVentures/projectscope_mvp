
%h1
  = @iteration.name

%div.col-md-6
  %h2
    Tasks
  %table.table
    %thead
      %tr
        -1.upto(@maxelem).each do |level|
          %th
            level #{level}
    %tbody
      - @level.each do |nodes|
        %tr{id: "task_#{@iteration}"}
          - nodes.each do |node|
            = render partial: 'task', locals: {node: node}

:javascript
  keep_log=false;