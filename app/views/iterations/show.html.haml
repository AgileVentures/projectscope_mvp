
%h1
  = @iteration.name

%div.col-md-6
  %h2
    Tasks
  %table.table
    %thead
      %tr
        %th
        -1.upto(@maxelem).each do |level|
          %th
            #{level}
    %tbody
      - @level.each do |nodes|
        %tr{id: "task_#{@iteration}"}
          %td
            Level #{@level.index nodes}
          - nodes.each do |node|
            -if node.nil?
              %td

            -else
              = render partial: 'task', locals: {node: node}

:javascript
  keep_log=false;